{
  "name": "webnuvola/laravel-active",
  "description": "Helper class for Laravel applications to get active class based on current route",
  "keywords": ["laravel", "active", "menu", "current"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fabio Cagliero",
      "email": "caglierofabio@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "illuminate/http": "5.8.*|^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "autoload": {
    "files": ["src/helpers.php"]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}