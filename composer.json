{
    "name": "webnuvola/laravel-active",
    "description": "Helper functions for Laravel applications to get active class based on current route",
    "keywords": ["laravel", "active", "menu", "current"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Cagliero",
            "email": "fabio@webnuvola.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0|^8.0",
        "phpunit/phpunit": "^9.5.27"
    },
    "autoload": {
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Webnuvola\\Laravel\\Active\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
