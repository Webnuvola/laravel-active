{
  "name": "webnuvola/laravel-active",
  "description": "Helper functions for Laravel applications to get active class based on current route",
  "keywords": ["laravel", "active", "menu", "current"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fabio Cagliero",
      "email": "caglierofabio@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/http": "^8.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "files": ["src/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Webnuvola\\Laravel\\Active\\Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
